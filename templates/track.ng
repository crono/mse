<md-grid-list 
    class="track" 
    md-cols="6" 
    md-rows="1" 
    md-row-height="1:1"
    md-gutter="2px">
  <md-grid-tile
      ng-repeat="v in [0, 1, 2, 3, 4, 5] track by $index">
    <div class="track-tile" ng-class="{current: v==value}">
      <h2>{{v}}</h2>
    </div>
  </md-grid-tile>
</md-grid-list>